<% content_for :head do %>
  <script src="https://apis.google.com/js/client:platform.js"></script>
<% end %>

<%= react_component "Setup", {
  action: config_setup_path,
  checkAvailabilityPath: config_check_availability_path,
  csrfToken: form_authenticity_token,
  installationId: params[:installation_id],
  gaClientId: Rails.application.secrets.google_client_id,
  gaScope: "https://www.googleapis.com/auth/analytics.readonly",
  gaDatePeriods: Config::DATE_PERIODS,
} %>
